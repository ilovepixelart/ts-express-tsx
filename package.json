{
  "name": "ts-express-tsx",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "tsc",
    "dev": "tsx watch src/bin/www.ts",
    "biome": "npx @biomejs/biome check",
    "biome:fix": "npx @biomejs/biome check --write .",
    "test": "vitest run --coverage",
    "test:open": "npm test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@biomejs/biome": "2.1.2",
    "cookie-parser": "1.4.7",
    "debug": "4.4.1",
    "dotenv": "17.2.0",
    "express": "5.1.0",
    "http-errors": "2.0.0",
    "mongoose": "8.16.4",
    "morgan": "1.10.1",
    "pug": "3.0.3",
    "ts-cache-mongoose": "1.7.4",
    "ts-migrate-mongoose": "4.0.9",
    "ts-patch-mongoose": "2.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.9",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.3",
    "@types/http-errors": "2.0.5",
    "@types/morgan": "1.9.10",
    "@types/node": "24.0.15",
    "@vitest/coverage-v8": "3.2.4",
    "mongodb-memory-server": "10.1.4",
    "open-cli": "8.0.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
}
